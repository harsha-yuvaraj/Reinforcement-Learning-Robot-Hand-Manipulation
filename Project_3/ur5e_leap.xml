<mujoco model="ur5e_leap_hand">

  <!-- 1) Combined Compiler and Options -->
  <compiler angle="radian" meshdir="assets" autolimits="true"/>
  <option integrator="implicitfast" cone="elliptic" impratio="10"/>

  <!-- 2) Combined <default>
         - UR5e defaults (class="ur5e")
         - Right Leap Hand defaults (class="right_leap") -->
  <default>

    <!-- UR5e Defaults -->
    <default class="ur5e">
      <material specular="0.5" shininess="0.25"/>
      <joint axis="0 1 0" range="-6.28319 6.28319" armature="0.1"/>
      <general gaintype="fixed" biastype="affine" ctrlrange="-6.2831 6.2831"
               gainprm="2000" biasprm="0 -2000 -400" forcerange="-150 150"/>

      <default class="size3">
        <default class="size3_limited">
          <joint range="-3.1415 3.1415"/>
          <general ctrlrange="-3.1415 3.1415"/>
        </default>
      </default>

      <default class="size1">
        <general gainprm="500" biasprm="0 -500 -100" forcerange="-28 28"/>
      </default>

      <default class="arm_visual">
        <geom type="mesh" contype="0" conaffinity="0" group="2"/>
      </default>

      <default class="arm_collision">
        <geom type="capsule" group="3"/>
        <default class="eef_collision">
          <geom type="cylinder"/>
        </default>
      </default>

      <site size="0.001" rgba="0.5 0.5 0.5 0.3" group="4"/>
    </default>

    <!-- Right Leap Hand Defaults -->
    <default class="right_leap">
      <site size="0.002" rgba="0.5 0.5 0.5 0.3" group="4"/>
      <joint axis="0 0 -1" armature="0.01"/>
      <position inheritrange="1" kp="20" dampratio="1"/>

      <default class="hand_visual">
        <!-- Using a custom material "black_leap" to avoid collision with UR5 "black" -->
        <geom group="2" type="mesh" contype="0" conaffinity="0"
              density="0" material="black_leap"/>
      </default>

      <default class="hand_collision">
        <geom group="3" type="mesh"/>
      </default>
    </default>

  </default>

